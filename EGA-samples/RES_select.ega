@ Search and select the resource item that contains Japanese text "開く".

=(res, RES_search());
=(text, u16fromu8("開く"));
foreach(item, res, (
    =(bin, RES_get_binary([](item, 0), [](item, 1), [](item, 2))),
    if(!=(find(bin, text), -(1)), (
        RES_select([](item, 0), [](item, 1), [](item, 2)),
        break()
    ))
));
